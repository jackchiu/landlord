<table>
  <th>
    <td>Group</td>
    <td>Number</td>
    <td>Floor</td>
    <td>Length</td>
    <td>Width</td>
    <td>State</td>
  </th>
  <% @rooms.each do |r| %>
  <tr>
    <td> <%= r.group.to_s %> </td>
    <td> <%= r.number.to_s %> </td>
    <td> <%= r.floor.to_s %> </td>
    <td> <%= r.length.to_s %> </td>
    <td> <%= r.width.to_s %> </td>
    <td> <%= r.state.to_s %> </td>
  </tr>
  <% end %>
</table>
  
